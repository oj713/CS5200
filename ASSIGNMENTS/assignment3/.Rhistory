crab1 <- c(12, 8, 4, 6, 10)
crab2 <- c(4, 20, 16, 5, 15)
dfcrabs <- as_tibble(crab1, crab2) |>
mutate(total = crab1 + crab2)
dfcrabs <- as_tibble(crab1, crab2) |>
mutate(total = crab1 + crab2,
expected1 = total * .4,
expected2 = total * .6)
dfcrabs
rbind(dfcrabs$expected1, dfcrabs$expected2)
c(rbind(dfcrabs$expected1, dfcrabs$expected2))
dfcrabs2 <- data.frame(expected = c(rbind(dfcrabs$expected1, dfcrabs$expected2)),
ob = c(rbind(crab1, crab2)))
dfcrabs2
df <- dfcrabs2 |>
mutate(o_minus_e = ob - expected,
oe_sq = o_minus_e ^2,
oe_sq_e = oe_sq/expected)
df
sum(df$oe_sq_e)
chisq.test(crab1, crab2)
chisq.test(data.frame(crab1, crab2))
